{
  "name": "anime-captcha",
  "version": "1.7.165",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leomotors/anime-captcha.git"
  },
  "author": "Leomotors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leomotors/anime-captcha/issues"
  },
  "homepage": "https://github.com/Leomotors/anime-captcha",
  "scripts": {
    "pre-build": "pnpm lm-config && node prebuild.js",
    "dev": "pnpm pre-build && vite dev --port 6912",
    "build": "pnpm pre-build && vite build && sh post-build.sh",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src && prettier --check \"src/**/*.{ts,svelte,html,scss}\"",
    "format": "eslint src --fix && prettier --write \"src/**/*.{ts,svelte,html,scss}\""
  },
  "engines": {
    "node": "^18.12.0",
    "pnpm": "^7"
  },
  "devDependencies": {
    "@leomotors/scripts": "^3.1.0",
    "@sveltejs/adapter-vercel": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@types/eslint": "^8.4.10",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "globby": "^13.1.3",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "sass": "^1.57.1",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-pwa": "^0.14.0"
  }
}
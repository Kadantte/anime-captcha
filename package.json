{
  "name": "anime-captcha",
  "version": "1.8.166",
  "type": "module",
  "author": "Leomotors",
  "license": "MIT",
  "scripts": {
    "pre-build": "pnpm lm-config && node prebuild.js",
    "dev": "pnpm pre-build && vite dev --port 6912",
    "build": "pnpm pre-build && vite build && sh post-build.sh",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src && prettier --check \"src/**/*.{ts,svelte,html,scss}\"",
    "format": "eslint src --fix && prettier --write \"src/**/*.{ts,svelte,html,scss}\""
  },
  "engines": {
    "node": "^18.12.0",
    "pnpm": "^8"
  },
  "packageManager": "pnpm@8.7.6",
  "devDependencies": {
    "@leomotors/scripts": "3.1.0",
    "@sveltejs/adapter-vercel": "3.0.3",
    "@sveltejs/kit": "1.25.1",
    "@types/eslint": "8.44.3",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-svelte": "2.33.2",
    "globby": "13.2.2",
    "postcss": "8.4.30",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "sass": "1.68.0",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-pwa": "0.16.5"
  }
}
